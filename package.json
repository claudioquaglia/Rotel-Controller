{
  "name": "rotel-ctrl",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Claudio Quaglia <cluadio.quaglia@codemania.it>",
  "main": "dist/index.js",
  "module": "dist/rotel-ctrl.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": "dist/index.js",
  "scripts": {
    "start": "tsdx watch --target node",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "commit": "git-cz",
    "release": "standard-version --no-verify --sign"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.10",
    "@types/mitm": "^1.3.3",
    "commitizen": "^4.1.5",
    "cz-conventional-changelog": "^3.2.1",
    "debug": "^4.1.1",
    "husky": "^4.2.5",
    "mitm": "^1.7.1",
    "standard-version": "^9.0.0",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
